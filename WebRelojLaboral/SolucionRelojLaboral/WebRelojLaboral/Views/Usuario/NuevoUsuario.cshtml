
@{
    ViewBag.Title = "NuevoUsuario";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>NuevoUsuario</h2>
<div class="container">
    <form class="needs-validation" >
        <div class="form-row">
            @*<div class="col-md-4 mb-3">
                <label for="validationCustom01">First name</label>
                <select name="cborol" id="cborol" class="form-control">
                    <option value="0">Seleccione una Provincia...</option>
                </select>
            </div>*@
            <div class="col-md-4 mb-3">
                <label for="cbolocal">Localización</label>
                <select name="cbolocal" id="cbolocalizacion" class="form-control">
                    <option value="0">Seleccione una Localizacion...</option>
                </select>
            </div>
            <div class="col-md-4 mb-3">
                <label for="nombres">Nombres</label>
                <input type="text" class="form-control" id="idNombreUsuario" placeholder="Ingrese su Nombre" required>
                
            </div>
            <div class="col-md-4 mb-3">
                <label for="apellidos">Apellidos</label>
                <input type="text" class="form-control" id="idApellidoUsuario" placeholder="Ingrese su Apellido" required>

            </div>
            <div class="col-md-4 mb-3">
                <label for="dni">DNI</label>
                <input type="text" class="form-control" id="idDNIUsuario" placeholder="Ingrese su DNI" required>

            </div>
            <div class="col-md-4 mb-3">
                <label for="cel1">Celular 1</label>
                <input type="text" class="form-control" id="idCel1Usuario" placeholder="Ingrese el numero de celular" required>

            </div>
            <div class="col-md-4 mb-3">
                <label for="nombres">Celular 2</label>
                <input type="text" class="form-control" id="idCel2Usuario" placeholder="Ingrese el numero de celular 2" required>

            </div>
            <div class="col-md-4 mb-3">
                <label for="ema">Email</label>
                <input type="text" class="form-control" id="idEmailUsuario" placeholder="Ingrese el email" required>

            </div>
            <div class="col-md-4 mb-3">
                <label for="dom">Domicilio</label>
                <input type="text" class="form-control" id="idDomicilioUsuario" placeholder="Ingrese el domicilio" required>

            </div>
            
            
           </div>
        <button class="btn btn-primary" type="button" id="idSubmitFormUsuario">Crear Usuario</button>
        @*<button class="btn btn-primary" id="idSubmitFormUsuario" type="button" data-toggle="collapse" data-target="#multiCollapseExample2" aria-expanded="false" aria-controls="multiCollapseExample2">Toggle second element</button>*@
    </form>
    <br />
    
    
    
    
    <div class="container"  style="margin-left:10%" >
        <div id="cardListaUsuarios">
           


        </div>
    </div>












</div>